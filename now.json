{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@now/node@canary",
      "config": {
        "maxLambdaSize": "25mb"
      }
    }
  ],
  "build": {
    "env": {
      "NODEJS_HELPERS": "0",
      "DATABASE_URL": "@database_url"
    }
  },
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ]
}
